                input=".real.00challenge"
                type="com.ibm.azportal.dre.struts.actions.LogonAction"
                validate="true">
            <set-property property="cancellable" value="true"/>
            <forward name="nogo" path=".real.06nogo" />
            <forward name="requiredInfo" path="/requiredInfo.do" />
            <forward name="submit.survey" path=".real.81survey" />
            <forward name="submit.help" path=".real.80help" />
        </action>

        <action path="/logoff"
                name="blank"
                scope="request"
                type="com.ibm.azportal.dre.struts.actions.LogoffAction" />

        <action path="/90agentTutorial" forward=".real.90agentTutorial"/>
        <action path="/services/91brokerTutorial" forward=".real.91brokerTutorial"/>
                
